<?xml version="1.0" encoding="utf-8"?>
<CustomNameFormatCollection xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <!-- Formats for generated proper names like 'starrikholm' etc -->

  <NameFormat> <!-- fantasy pseudo-english names inspired by the first law trilogy naming conventions -->
    <BlockedTerrain>SEA</BlockedTerrain>
    <BlockedTerrain>THRONE</BlockedTerrain>
    <Segment>START_A</Segment>
    <Segment>MID_A</Segment>
    <Segment>END_A</Segment>
  </NameFormat>

  <NameFormat>
    <BlockedTerrain>SEA</BlockedTerrain>
    <BlockedTerrain>THRONE</BlockedTerrain>
    <Segment>MID_A</Segment>
    <Segment>END_A</Segment>
  </NameFormat>

  <NameFormat>
    <BlockedTerrain>SEA</BlockedTerrain>
    <BlockedTerrain>THRONE</BlockedTerrain>
    <Segment>START_SHORT_A</Segment>
    <Segment>END_A</Segment>
  </NameFormat>

  <NameFormat>
    <Segment>START_A</Segment>
    <Segment>MID_A</Segment>
    <Segment>OWNERSHIP_END_A</Segment>
    <Segment>SPACE</Segment>
    <Segment>NOUN</Segment>
  </NameFormat>

  <NameFormat>
    <Segment>MID_A</Segment>
    <Segment>OWNERSHIP_END_A</Segment>
    <Segment>SPACE</Segment>
    <Segment>NOUN</Segment>
  </NameFormat>

  <NameFormat>
    <Segment>START_SHORT_A</Segment>
    <Segment>OWNERSHIP_END_A</Segment>
    <Segment>SPACE</Segment>
    <Segment>NOUN</Segment>
  </NameFormat>

  <NameFormat> 
    <BlockedTerrain>SEA</BlockedTerrain>
    <BlockedTerrain>THRONE</BlockedTerrain>
    <Segment>START_A</Segment>
    <Segment>MID_A</Segment>
    <Segment>END_A</Segment>
    <Segment>SPACE</Segment>
    <Segment>NOUN</Segment>
  </NameFormat>

  <NameFormat> 
    <BlockedTerrain>SEA</BlockedTerrain>
    <BlockedTerrain>THRONE</BlockedTerrain>
    <Segment>START_SHORT_A</Segment>
    <Segment>END_A</Segment>
    <Segment>SPACE</Segment>
    <Segment>NOUN</Segment>
  </NameFormat>

  <NameFormat> <!-- fantasy royal-ish names inspired by the first law trilogy naming conventions -->
    <BlockedTerrain>SEA</BlockedTerrain>
    <BlockedTerrain>WASTE</BlockedTerrain>
    <Segment>START_B</Segment>
    <Segment>MID_B</Segment>
    <Segment>END_B</Segment>
  </NameFormat>
 
</CustomNameFormatCollection>